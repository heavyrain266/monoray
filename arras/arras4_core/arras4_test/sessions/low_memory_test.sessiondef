{
    "name": "simple_test",
    "description": "computation echos messages back to client",
    "computations": {
        "(client)": {
            "messages": {
                "testcomp": "*"
            }
        }, 
        "testcomp": {
            "dso": "libarras4_testcomputation.so", 
            "entry": "yes", 
            "forward":true,
            "requirements": {
                "computationAPI": "4.x",
                "rez_packages": "arras4_test-4",
                "resources": {
                    "cores": 1.0,
                    "memoryMB": 2048
                }
            },
            "messages": {
                "(client)": "*" 
            }
        }
    }
}
