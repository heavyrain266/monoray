{
    "name": "chunking_test",
    "description": "sets chunking to 1023 bytes to make testing easier",
    "computations": {
        "(client)": {
            "messages": {
                "testcomp": "*"
            }
        }, 
        "testcomp": {
            "dso": "libarras4_testcomputation.so", 
            "entry": "yes", 
            "forward":true,
            "minChunkingBytes":1023,
            "chunkSizeBytes":1024,
            "requirements": {
                "computationAPI": "4.x",
                "rez_packages": "arras4_test-4",
                "resources": {
                    "cores": 1.0,
                    "memoryMB": 16384
                }
            },
            "messages": {
                "(client)": "*" 
            }
        }
    }
}
